%span.form_label Low Point Threshold
%input.low-range{:type => 'number','index' => '{{index}}','name' => 'low_range', 'ng-model' => 'element.low_range'}
%span.form_label High Point Threshold
%input.high-range{:type => 'number', 'name' => 'high_range', 'ng-model' => 'element.high_range', 'ng-disabled' => 'index > 0'}
